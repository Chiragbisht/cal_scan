{
  "expo": {
    "name": "CalScan",
    "slug": "calscan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "calscan",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bisht.calscan",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan food items for nutritional analysis."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#f8f8f8"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA"
      ],
      "package": "com.bisht.calscan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "03556fc4-fc47-4902-ad2b-dc05384cd35b"
      }
    },
    "owner": "bisht"
  }
}
